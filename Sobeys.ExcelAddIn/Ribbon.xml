<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
    <tabs>
      <tab id="Sobeys" getLabel="GetLabel">
        <group id="QuickActionGroup" getLabel="GetLabel">
			<button size="large" id="SuperCopy" getLabel="GetLabel" getScreentip="GetLabel" onAction="OnWorkbookAction" getEnabled="GetWorkbookEnabled" imageMso="Copy" />
        </group>
		  <group id="AboutGroup" getLabel="GetLabel">
			  <toggleButton size="large" id="Settings" getLabel="GetLabel" getScreentip="GetLabel" getPressed="GetWorkbookPressed" getEnabled="GetWorkbookEnabled" onAction="OnWorkbookPressedAction" imageMso="ComAddInsDialog" />
			  <button size="large" id="About" getLabel="GetLabel" getScreentip="GetLabel" onAction="OnAction" getEnabled="GetEnabled" imageMso="About" />
		  </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>

